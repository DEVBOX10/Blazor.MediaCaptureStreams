@if (ConstrainULong.Value is ulong { } ulongValue)
{
    <code>@ulongValue</code>
}
else if (ConstrainULong.Value is ConstrainULongRange { } constrainULongRange)
{
    <ConstrainULongRangePresenter ConstrainULongRange="constrainULongRange" />
}
